<span class="pl-k">import</span> <span class="pl-smi">org.jenkinsci.plugins.rolestrategy.permissions.PermissionHelper</span>

<span class="pl-k">Jenkins</span> jenkins <span class="pl-k">=</span> <span class="pl-k">Jenkins</span><span class="pl-k">.</span>get()
<span class="pl-k">def</span> rbas <span class="pl-k">=</span> <span class="pl-k">new</span> <span class="pl-k">RoleBasedAuthorizationStrategy</span>()

<span class="pl-c"><span class="pl-c">/*</span> create admin role <span class="pl-c">*/</span></span>
